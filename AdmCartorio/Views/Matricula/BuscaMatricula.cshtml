            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Busca de Matriculas</h4>
            </div>
            <div class="modal-body">
                <fieldset>
                    <div class="row">
                        <div class="col-sm-5">
                            <label class="margin-top-10">CPF</label>
                            <input placeholder="Digite o CPF" class="form-control" />
                            @*@Html.TextBoxFor(model => model.NomeModelo, null, new { @class = "form-control", @placeholder = "Digite o nome do modelo" })
                                @Html.ValidationMessageFor(model => model.NomeModelo, null, new { @class = "text-danger" })*@
                        </div>
                        <div class="col-sm-5">
                            <label class="col-form-label margin-top-10">Digite a matrícula</label>
                            <input placeholder="Digite o Nº da Matrícula" class="form-control" />

                            @*@Html.DropDownListFor(model => model.NaturezaArquivoModeloDocx, (ViewBag.NaturezaArquivoModeloDocx as SelectList), new { @class = "form-control" })
