                        </div>
                        <div class="col-sm-1">
                            <br />
                            <button class="btn btn-default margin-top-10 margin-left-5">
                                <i class="glyphicon glyphicon-search"></i>
                            </button>
                        </div>
                    </div>
                </fieldset>
                
                    <table id="tabelaNatureza" class="table table-bordered table-hover dataTable no-footer has-columns-hidden" role="grid" aria-describedby="dt_basic_info" style="width: 100%;">
                        <thead>
                            <tr role="row">
                                <th rowspan="1" colspan="1">
                                    Nº Matrícula
                                </th>
                                <th data-class="expand" class="expand sorting" tabindex="0" aria-controls="dt_basic" rowspan="1" colspan="1" aria-label="Nome: activate to sort column ascending">
                                    Nome do imóvel
                                </th>
                                <th rowspan="1" colspan="1">
                                    Endereço
                                </th>
                                <th rowspan="1" colspan="1">
                                    Proprietário atual
                                </th>
                                <th rowspan="1" colspan="1">
                                    Ação
                                </th>
                        </thead>
                        <tbody>
                            <!-- MOCADO-->
                            <tr role="row" id="1">
                                <td>125515</td>
                                <td>Imovel do Pedro</td>
                                <td>Rua Pires da silva</td>
                                <td>Pedro Pires</td>
                                <td>
                                    <form role="form">
                                        <button id="btnUsar1"
                                                class="btn btn-success" type="submit">Confirmar</button>
                                    </form>

                                </td>
                            </tr>
                            <tr role="row" id="2">
                                <td>289852</td>
                                <td>Johny's Property</td>
                                <td>Caucaia do alto - CE</td>
                                <td>Johny Walker</td>
                                <td>
