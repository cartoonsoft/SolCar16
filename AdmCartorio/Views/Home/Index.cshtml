<!-- MAIN CONTENT -->
@{ 
    ViewBag.Title = "Home";
    ViewBag.SubTitle = "Index";
}
<div id="content">
    <div class="row">
        <div class="col-xs-12 col-sm-7 col-md-7 col-lg-4">
            <h1 class="page-title txt-color-blueDark">

                <!-- PAGE HEADER -->
                <i class="fa-fw fa fa-home"></i>
                @ViewBag.Title
                <span>
                    >
                    @ViewBag.SubTitle
                </span>
            </h1>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
            @{
                if (TempData["AvisoEmail"] != null)
                {
                    <h4 class="text-danger">@TempData["AvisoEmail"]</h4>
                }
            }
        </div>
    </div>
</div>